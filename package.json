{
  "name": "thesis-project",
  "version": "1.0.0",
  "private": true,
  "description": "A tool for improving in-class student-teacher interactions",
  "main": "index.js",
  "scripts": {
    "start": "webpack --progress && node index.js",
    "start-dev": "npm run build && npm run MYjshint && ((sleep 36 && npm test) & nodemon index.js)",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run oldBuild",
    "test": "mocha server/spec/**/*.js server/database/spec/**/*.js server/authentication/spec/**/*.js",
    "MYjshint": "jshint **/*.js --show-non-errors",
    "oldBuild": "npm run copy && npm run concat",
    "copy": "cp -r client/text dist/ && cp -r client/thumbs dist/ && cp -r client/protected dist/ && cp -r client/authentication dist/auth && cp -r client/text dist/text",
    "concat": "(cat node_modules/jquery/dist/jquery.min.js; echo '\n'; cat node_modules/underscore/underscore-min.js; echo '\n'; cat node_modules/socket.io-client/socket.io.js; echo '\n';) >> dist/vendors.js"
  },
  "engines" : { "npm" : "2.15.3" }, 
  "author": "Louis Buchbinder, Ben Cheatham, Steven Sullivan, and Jason Wu",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.0",
    "bootstrap": "^4.0.0-alpha.2",
    "bootstrap-loader": "^1.0.10",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.23.1",
    "express": "4.13.4",
    "express-jwt": "^3.3.0",
    "html-webpack-plugin": "2.15.0",
    "humps": "^1.0.0",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.3",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.11.1",
    "node-sass": "^3.4.2",
    "normalizr": "^2.0.1",
    "postcss-loader": "^0.8.2",
    "react": "0.14.8",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "socket.io": "^1.4.5",
    "sqlite3": "^3.1.3",
    "socket.io-client": "^1.4.5",
    "style-loader": "^0.13.1",
    "tether": "^1.2.0",
    "underscore": "1.8.3",
  
    "axios": "^0.10.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "jshint": "^2.9.2",
    "mocha": "^2.4.5",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "webpack": "1.12.14",
